# -*- mode: yaml; -*-
# Slack Notification plugin
---
webhook_url: "<%= @webhook %>"
channel: "<%= @channel %>"
puppetconsole: "<%= @puppetconsole %>"
<% unless @username.empty? -%>
username: "<%= @username %>"
<% end -%>
<% unless @icon_url.empty? -%>
icon_url: "<%= @icon_url %>"
<% end -%>
<% unless @icon_emoji.empty? -%>
icon_emoji: "<%= @icon_emoji %>"
<% end -%>
